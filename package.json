  "scripts": {
    "dev:chat": "nx run chat:serve --host=127.0.0.1 --port=4200",
    "dev:admin": "nx run admin:serve --host=127.0.0.1 --port=4200",
    "dev:discover": "nx run discover:serve",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "nx format:check",
    "format:fix": "nx format:write",
    "build:chat": "cross-env GENERATE_SOURCEMAP=false nx build chat --prod --baseHref=/",
    "build:admin": "cross-env GENERATE_SOURCEMAP=false nx build admin --prod --baseHref=/",
    "build:discover": "cross-env GENERATE_SOURCEMAP=false nx build discover --prod --baseHref=/",
    "build-stats:chat": "nx run chat:build --statsJson",
    "bundle-analyzer:chat": "webpack-bundle-analyzer ./dist/apps/chat/stats.json",
    "analyze:chat": "yarn build-stats:chat && yarn bundle-analyzer:chat",
    "serve:chat": "serve -s ./dist/apps/chat",
    "dev:mobile": "npx nx start mobile",
    "dev:android": "npx nx run-android mobile",
    "dev:ios": "npx nx run-ios mobile",
    "build:android": "npx nx build-android mobile",
    "build:ios": "npx nx build-ios mobile",
    "postinstall": "electron-builder install-app-deps",
    "dev:desktop": "nx run desktop:serve",
    "install:desktop": "cd apps/desktop && yarn",
    "build:desktop:app": "nx run desktop:build",
    "build:desktop:renderer": "cross-env BUILD_TARGET='electron' nx build chat --prod --baseHref=./",
    "build:desktop": "yarn install:desktop && yarn build:desktop:app && yarn build:desktop:renderer",
    "package:desktop": "nx run desktop:package",
    "make:desktop": "nx run desktop:make",
    "release:desktop": "yarn build:desktop && yarn package:desktop && yarn make:desktop",
    "prepare": "husky"
  },
